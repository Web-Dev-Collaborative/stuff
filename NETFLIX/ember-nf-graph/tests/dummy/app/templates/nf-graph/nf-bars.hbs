{{#nf-graph width=500
           height=300
           paddingRight=10
           paddingTop=10
            xScaleType="ordinal"
            yMinMode="fixed"
            yMin=0
            showLanes=true
            showFrets=true
            as |nf|
            }}
  {{#nf.graph as |graph|}}

    {{graph.bars data=model.menuItemStockCount
                trackingMode="snap-last"
        trackedData=tracked}}

  {{/nf.graph}}

  {{#nf.y-axis as |tick|}}
    <text>{{tick.value}}</text>
  {{/nf.y-axis}}

  {{#nf.x-axis as |tick|}}
    <text>{{tick.value}}</text>
  {{/nf.x-axis}}

 {{/nf-graph}}

 <text>{{tracked.x}} {{tracked.y}}</text>
